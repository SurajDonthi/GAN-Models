{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Training",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/gan_models/main.py",
      "console": "integratedTerminal",
      "pythonArgs": ["-W", "ignore::UserWarning::0"],
      "args": [
        "--model",
        "gan",
        "--data_dir",
        "data/",
        "--description",
        "Training the model with Vanilla GAN",
        "--weights_summary",
        "full",
        "--gpus",
        "1",
        "--git_tag",
        "True",
        "--debug",
        "False",
        "--max_epochs",
        "2"
      ]
    }
    {
      "name": "Debugging",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/gan_models/main.py",
      "console": "integratedTerminal",
      "pythonArgs": ["-W", "ignore::UserWarning::0"],
      "args": [
        "--model",
        "gan",
        "--data_dir",
        "data/",
        "--description",
        "Testing the model initially",
        "--weights_summary",
        "full",
        "--gpus",
        "1",
        "--git_tag",
        "False",
        "--debug",
        "True",
        "--limit_train_batches",
        "0.05",
        "--max_epochs",
        "2"
      ]
    }
  ]
}
