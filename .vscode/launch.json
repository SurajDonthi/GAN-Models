  "version": "0.2.0",
  "configurations": [
    {
      "name": "Training",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/gan_models/main.py",
      "console": "integratedTerminal",
      "pythonArgs": ["-W", "ignore::UserWarning::0"],
      "args": [
        "--model",
        "gan",
        "--data_dir",
        "data/",
        "--description",
        "Vanilla GAN; Accuracy removed from logging; 100 epochs",
        "--weights_summary",
        "full",
        "--train_batchsize",
        "128",
        "--gpus",
        "1",
        "--git_tag",
        "True",
        "--debug",
        "False",
        "--max_epochs",
        "100"
      ]
    },
    {
      "name": "Debugging",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/gan_models/main.py",
      "console": "integratedTerminal",
      "pythonArgs": ["-W", "ignore::UserWarning::0"],
      "args": [
        "--model",
        "gan",
        "--data_dir",
        "data/",
        "--description",
        "Testing the model initially",
        "--weights_summary",
        "full",
        // "--train_batchsize",
        // "64",
        "--gpus",
        "1",
        "--git_tag",
        "False",
        "--debug",
        "False",
        "--d_skip_batch",
        "1",
        "--limit_train_batches",
        "0.05",
        "--max_epochs",
        "10",
        "--profiler",
        "True"
      ]
    }
  ]
}
